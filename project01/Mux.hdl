/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;
    
    
    PARTS:
    Not(in=a, out=notA );
    And(a=notA , b=sel , out=notAandSel );
    And(a=b , b=sel , out=bAndSel );
    Or(a=notAandSel , b=bAndSel , out=out );
}
